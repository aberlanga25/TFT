<1@Popup>:
    size_hint: None, None
    size: dp(200), dp(100)
    title: 'Needlessly Large Rod'
    auto_dismiss: False
    BoxLayout:
        padding: dp(10)
        MDLabel:
            text:
                "+21% Spell Damage"
            markup: True
            size_hint_y: None
            height: self.texture_size[1]
            color: 1, 1, 1, 1
